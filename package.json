  "scripts": {
    "local": "set PORT=9000 && lessc --npm-import=\"prefix=~\" src/less/index.less src/css/index.css && node local.js",
    "lint-es": "eslint src/js/**/*.js src/__tests__/**/*.js --no-ignore",
    "lint-es-fix": "eslint src/js/**/*.js src/__tests__/**/*.js --no-ignore --fix",
    "start": "react-scripts start",
    "build": "lessc --npm-import=\"prefix=~\"  src/less/index.less src/css/index.css && react-scripts build",
    "buildless": "lessc --npm-import=\"prefix=~\" src/less/index.less src/css/index.css",
    "test": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test-fix": "react-scripts test --env=jsdom -u",
    "test-watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/js/Config.js",
      "!<rootDir>/local.js",
      "!<rootDir>/decorator.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint-config-airbnb": "^17.1.0",
    "jest-fetch-mock": "^1.6.5",
    "less-watch-compiler": "^1.11.3",
    "nav-frontend-core": "^4.0.9",
    "nav-frontend-lenker-style": "^0.2.6",
    "react-test-renderer": "^16.4.2",
    "reload": "^3.0.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-class-properties",
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
