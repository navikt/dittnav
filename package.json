    "react-test-renderer": "^16.12.0",
    "react-scripts": "^3.4.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 2
