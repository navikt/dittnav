  "scripts": {
    "local": "PORT=9000 node local.js",
    "local-win": "set PORT=9000 && lessc --npm-import=\"prefix=~\" src/less/index.less src/css/index.css && node local.js",
    "lint-es": "eslint src/js/**/*.js src/__tests__/**/*.js --no-ignore",
    "lint-es-fix": "eslint src/js/**/*.js src/__tests__/**/*.js --no-ignore --fix",
    "start": "react-scripts start",
    "build": "lessc --npm-import=\"prefix=~\" src/less/index.less src/css/index.css && react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --watchAll=false",
    "test-fix": "react-scripts test --env=jsdom -u",
    "test-watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/js/Config.js",
      "!<rootDir>/local.js",
      "!<rootDir>/decorator.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint-config-airbnb": "^18.0.0",
    "jest-fetch-mock": "^2.1.2",
    "less-watch-compiler": "^1.11.3",
    "nav-frontend-core": "^4.0.10",
    "nav-frontend-lenker-style": "^0.2.20",
    "react-test-renderer": "^16.9.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 2
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2
        }
      ]
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 10",
    "not op_mini all"
  ]
}
