{
  "name": "dittnav",
  "version": "0.1.0",
  "private": false,
  "author": "NAV",
  "license": "MIT",
  "homepage": "/dittnav",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "frontshell": "git+https://git@github.com/navikt/frontshell.git#1.2.3",
    "https": "^1.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-intl": "^2.4.0",
    "react-scripts": "2.0.0",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build-react": "less-watch-compiler --run-once --main-file=index.less src/less src/css && react-scripts build",
    "build-backend": "babel src -d dist",
    "copy-static": "mkdir dist/static && cp -r build/static/css dist/static/css && cp -r build/static/media dist/static",
    "start": "node dist/backend.js",
    "build": "npm run clean && npm run build-react && npm run build-backend && npm run copy-static",
    "docker-build": "docker build . -t dittnav",
    "docker-start": "docker run --name dittnav -p 8080:8080 -t -d dittnav",
    "docker-kill": "npm run docker-stop && npm run docker-rm",
    "docker-rm": "docker rm dittnav",
    "docker-stop": "docker stop dittnav",
    "test": "react-scripts test --env=jsdom --coverage",
    "test-watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "jest-fetch-mock": "^1.6.5",
    "less-watch-compiler": "^1.11.3",
    "nav-frontend-core": "^4.0.5",
    "nav-frontend-lenker-style": "0.2.6",
    "react-test-renderer": "^16.4.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
