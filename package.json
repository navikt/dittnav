{
  "name": "dittnav",
  "version": "0.1.0",
  "private": false,
  "author": "NAV",
  "license": "MIT",
  "homepage": "/person/dittnav",
  "dependencies": {
    "frontshell": "git+https://git@github.com/navikt/frontshell.git#1.2.3",
    "htmllint-cli": "0.0.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-intl": "^2.4.0",
    "react-scripts": "2.1.3",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "lint-html": "htmllint public/index.html",
    "lint-es": "eslint src/js/**/*.js src/__tests__/**/*.js --no-ignore",
    "docker-build": "docker build . -t dittnav",
    "docker-start": "docker run --name dittnav -p 8080:8080 -t -d dittnav",
    "docker-kill": "npm run docker-stop && npm run docker-rm",
    "docker-rm": "docker rm dittnav",
    "docker-stop": "docker stop dittnav",
    "mock-api": "cp -r src/mock-api build/",
    "frontshell-dev": "NODE_ENV=development decorated",
    "server": "decorated",
    "start": "react-scripts start",
    "build": "less-watch-compiler --run-once --main-file=index.less src/less src/css && react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test-watch": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/src/js/Config.js"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "jest-fetch-mock": "^1.6.5",
    "less-watch-compiler": "^1.11.3",
    "nav-frontend-core": "^4.0.5",
    "nav-frontend-lenker-style": "0.2.6",
    "react-test-renderer": "^16.4.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
